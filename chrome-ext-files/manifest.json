{
  "manifest_version": 2,
  "name": "Still",
  "short_name": "Still",
  "description": "A simple, minimalist new tab extension.",
  "version": "1.0.2",
  "icons": {
    "16": "zen.png",
    "48": "zen.png",
    "128": "zen.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://maps.googleapis.com; object-src 'self'",
  "permissions": [
    "geolocation"
  ]
}
